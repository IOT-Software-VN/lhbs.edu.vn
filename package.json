{
  "name": "lhbs-monorepo",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules",
    "turbo:login": "turbo login",
    "turbo:link": "turbo link",
    "turbo:cache:clear": "rm -rf .turbo/cache",
    "turbo:cache:test": "pnpm run turbo:cache:clear && pnpm run build",
    "serve:static": "node scripts/serve-static.js"
  },
  "devDependencies": {
    "turbo": "^2.7.2",
    "typescript": "^5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20"
  }
}
